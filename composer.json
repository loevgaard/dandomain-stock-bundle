{
    "name": "loevgaard/dandomain-stock-bundle",
    "description": "Symfony bundle to handle stock i Dandomain, especially stock movements",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "doctrine/doctrine-bundle": "^1.6",
        "loevgaard/dandomain-stock-entities": "dev-master",
        "symfony/framework-bundle": "^3.3||^4.0",
        "symfony/yaml": "^3.0||^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.7||^6.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Loevgaard\\DandomainStockBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "./vendor/bin/php-cs-fixer fix --verbose --dry-run",
        "fix-style": "./vendor/bin/php-cs-fixer fix --verbose"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
